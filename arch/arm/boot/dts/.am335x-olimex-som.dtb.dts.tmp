  mailbox: mailbox@480C8000 {
   compatible = "ti,omap4-mailbox";
   reg = <0x480C8000 0x200>;
   interrupts = <77>;
   ti,hwmods = "mailbox";
   #mbox-cells = <1>;
   ti,mbox-num-users = <4>;
   ti,mbox-num-fifos = <8>;
   mbox_wkupm3: wkup_m3 {
